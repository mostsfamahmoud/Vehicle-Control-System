#include "Vehicle.h"

int main(void) {

	Vehicle v1;

	v1.AC = OFF;
	v1.engine = ON;
	v1.engineTempController = OFF;
	v1.speed = 50;
	v1.engineTemp = 90;
	v1.roomTemp = 35;





	return EXIT_SUCCESS;
}
